print('Hello World!')
print(3 + 67)
print(7 * 6)
print()
print('The End')
print("Hello" + " World")

greeting = "Hello"
name = "Cody"
print(greeting + ' ' + name)

#########use single or double quote
#########comments are always useful and not picked up by computer

greeting = "Hello"
name = input("Please Enter your name! ")
print(greeting + " " + name)

splitString = "This Variable Is\nSplit\nover\nmany\nlines"
print(splitString)

tString = "1\t2\t\t3"
print(tString)

secondSplitString = """String
Split
over lines of code"""
print(secondSplitString)
